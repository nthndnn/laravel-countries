{
    "name": "nathandunn/laravel-countries",
    "type": "library",
    "authors": [
        {
            "name": "Nathan Dunn",
            "email": "github@nathandunn.uk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/bus": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.0",
        "packagedby/model-repositories": "^1.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23"
    },    
    "autoload": {
        "psr-4": {
            "NathanDunn\\Countries\\": "src",
            "NathanDunn\\Countries\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NathanDunn\\Countries\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
